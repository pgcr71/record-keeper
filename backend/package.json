  "devDependencies": {
    "@types/node": "^8.0.29",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "typeorm": "0.2.29",
    "reflect-metadata": "^0.1.10",
    "mysql": "^2.14.1",
    "express": "^4.15.4",
    "body-parser": "^1.18.1"
  }
}
