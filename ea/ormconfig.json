{
  "type": "sqlite",
  "database": "data/database.sqlite",
  "logging": false,
  "entities": [
    "data/entities/**/*.entity.ts"
  ],
  "migrations": [
    "data/migrations/**/*.ts"
  ],
  "subscribers": [
    "data/subscribers/**/*.subscriber.ts"
  ],
